#
# Sets Prezto options.
#

zstyle ':prezto:load' pmodule \
  'environment' \
  'history' \
  'directory' \
  'spectrum' \
  'completion' \
  'history-substring-search' \
  'autosuggestions' \
  'prompt'

zstyle ':prezto:module:autosuggestions:color' found ''

zstyle ':prezto:module:prompt' theme 'ryosuke'


